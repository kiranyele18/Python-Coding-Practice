    "n = int(input(\"Enter number : \"))\n",
    "\n",
    "rev = 0\n",
    "\n",
    "while(n>0):\n",
    "    dig = n % 10\n",
    "    rev = rev * 10 + dig\n",
    "    n = n//10\n",
    "print(\"The reversed Number is : \", rev)"
